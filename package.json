{
  "name": "next_blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "test": "jest src/__tests__/.*/*.test.tsx?$",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook -o build/storybook",
    "build-storybook": "build-storybook",
    "storyshots": "jest src/storybook.test.ts",
    "puppeteer-storyshots": "jest storyshots/puppeteer-storyshots*.test.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "yarn test"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.1",
    "@material-ui/icons": "^4.9.1",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-docs": "^6.1.10",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/addon-storyshots": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "dayjs": "^1.9.6",
    "next": "^10.0.3",
    "next-pwa": "^5.0.0",
    "pwacompat": "^2.0.17",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "sass": "^1.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-essentials": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storyshots-puppeteer": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.10",
    "@types/puppeteer": "^5.4.2",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-loader": "^8.2.2",
    "cypress": "^6.1.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-react": "^7.21.5",
    "husky": "4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "10.3.0",
    "prettier": "^2.2.1",
    "puppeteer": "^5.5.0",
    "sass-loader": "^10.1.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
